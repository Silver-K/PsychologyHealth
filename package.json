  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "dayjs": "^1.11.13",
    "docxtemplater": "^3.66.2",
    "nanoid": "^5.1.5",
    "pizzip": "^3.2.0"
  }
}
